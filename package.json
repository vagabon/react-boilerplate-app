{
  "name": "@vagabond-inc/react-boilerplate-app",
  "version": "1.1.2d",
  "description": "Boilerplate App for React (Auth / Admin / News)",
  "private": false,
  "scripts": {
    "lint": "eslint  \"{src/**/*,*}.{js,ts,jsx,tsx}\"",
    "tsc": "tsc",
    "test": "jest  ",
    "test:json": "npm run test -- --json --outputFile=src/jest-test-results.json --coverage",
    "coverage": "npm run test -- --coverage ",
    "format": "npm run lint && npm run tsc",
    "ci": "npm run format && npm run coverage -- --testResultsProcessor jest-sonar-reporter",
    "sonar": "npm run ci && npx sonar-scanner -Dsonar.login=$SONAR_TOKEN",
    "copy:asset": "cp -r src/assets dist/ && cp src/setupTests-mock-md.js dist/ && cp src/setupTests-mock-app.js dist/",
    "copy:setup-tests": "cp node_modules/@vagabond-inc/react-boilerplate-md/dist/setupTests.js src/setupTests-md.js",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:dev": "npm run build -- --watch",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "npm i && npm run format && npm run test:json && npm run build"
  },
  "dependencies": {
    "@vagabond-inc/react-boilerplate-md": "1.1.2d",
    "@greatsumini/react-facebook-login": "~3.3.3",
    "@mui/x-charts": "~7.5.0",
    "@react-oauth/google": "~0.12.1",
    "@reduxjs/toolkit": "~2.2.5",
    "axios": "~1.6.8",
    "formik": "~2.4.6",
    "notistack": "~3.0.1",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "react-ga4": "~2.1.0",
    "react-google-recaptcha": "~3.1.0",
    "react-helmet-async": "~2.0.5",
    "react-redux": "~9.1.2",
    "react-share": "~5.1.0",
    "yup": "~1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.24.5",
    "@babel/preset-react": "~7.24.1",
    "@babel/preset-typescript": "~7.24.1",
    "@rollup/plugin-babel": "~6.0.4",
    "@rollup/plugin-commonjs": "~25.0.7",
    "@rollup/plugin-json": "~6.1.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@rollup/plugin-typescript": "~11.1.6",
    "@storybook/addon-essentials": "~8.1.1",
