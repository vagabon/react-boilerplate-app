{
  "username": {
    "required": true,
    "min": 5,
    "max": 50,
    "regexp": "^[a-zA-Z0-9_-]{5,50}(?! )+"
  },
  "password": {
    "required": true,
    "max": 50,
    "regexp": "^(?=.*[A-Za-z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,}$",
    "regexpError": "ERRORS:FORMAT_PASSWORD"
  }
}
