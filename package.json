  },
  "files": [
    "dist"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
